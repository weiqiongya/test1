    </servlet>
    <servlet-mapping>
        <servlet-name>ClassFind</servlet-name>
        <url-pattern>/ClassFind</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>Delinfo</servlet-name>
